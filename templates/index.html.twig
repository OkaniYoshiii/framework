{% extends 'base.html.twig' %}

{% block main %}
    {{ form.render() }}

    <hr>

    {{ form.render('start') }}
        {% for input in form.inputs %}
            {{ input.label.render() }}
            {{ input.render('self') }}
        {% endfor %}
        {{ form.render('submit') }}
    {{ form.render('end') }}
{% endblock %}